    },
    "featuresList": {
      "type": "component",
      "repeatable": true,
      "component": "services-features.item"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "longDescription": {
      "type": "richtext",
      "required": true
    },
    "brnText": {
      "type": "string",
